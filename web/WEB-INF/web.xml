<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>StudentLogin</servlet-name>
        <servlet-class>servlet.StudentLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TeacherLogin</servlet-name>
        <servlet-class>servlet.TeacherLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ManagerLogin</servlet-name>
        <servlet-class>servlet.ManagerLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StudSelectCourse</servlet-name>
        <servlet-class>servlet.StudSelectCourse</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StudUndoSelected</servlet-name>
        <servlet-class>servlet.StudUndoSelected</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogOff</servlet-name>
        <servlet-class>servlet.LogOff</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TeacherCourseSelectStudent</servlet-name>
        <servlet-class>servlet.TeacherCourseSelectStudent</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TeacherCommitGrade</servlet-name>
        <servlet-class>servlet.TeacherCommitGrade</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StudentAdd</servlet-name>
        <servlet-class>servlet.StudentAdd</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TeacherAdd</servlet-name>
        <servlet-class>servlet.TeacherAdd</servlet-class>
    </servlet>


    <servlet-mapping>
        <servlet-name>StudentLogin</servlet-name>
        <url-pattern>/studentLogin.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TeacherLogin</servlet-name>
        <url-pattern>/teacherLogin.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ManagerLogin</servlet-name>
        <url-pattern>/managerLogin.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StudSelectCourse</servlet-name>
        <url-pattern>/studSelectCourse.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StudUndoSelected</servlet-name>
        <url-pattern>/studUndoSelected.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogOff</servlet-name>
        <url-pattern>/logoff.do</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>TeacherCourseSelectStudent</servlet-name>
        <url-pattern>/teachercourseSelectStudent.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TeacherCommitGrade</servlet-name>
        <url-pattern>/teacherCommitGrade.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StudentAdd</servlet-name>
        <url-pattern>/studentAdd.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TeacherAdd</servlet-name>
        <url-pattern>/teacherAdd.do</url-pattern>
    </servlet-mapping>
</web-app>